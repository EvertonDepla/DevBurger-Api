{
  "env": {
      "es2021": true,
      "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
    "class-methods-use-this": "off",
    "camelcase": "off",
    "no-param-reassign":"off",
    "prettier/prettier": ["error",{
      "endOfLine": "auto"}
  ]
}
}